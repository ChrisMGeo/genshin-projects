      "default": "./dist/generators/artifact.js"
    },
    "./character-info": {
      "types": "./src/generated/character-info.ts",
      "default": "./dist/generated/character-info.js"
    },
    "./weapon-info": {
      "types": "./src/generated/weapon-info.ts",
      "default": "./dist/generated/weapon-info.js"
    },
    "./artifact-info": {
      "types": "./src/generated/artifact-info.ts",
      "default": "./dist/generated/artifact-info.js"
